{
    "paths": {
        "topdir": "../..",
        "agecsv": "config/ages.csv",
        "imgcsv": "config/term-subjects.csv",
        "tmpdir": "output/term/c35",
        "outdir": ""
    },
    "images": {
        "default": "t2w",
        "ref": "global/roi/domain.nii.gz",
        "t1w": {
            "prefix": "images/t1w/",
            "suffix": ".nii.gz",
            "interp": "linear",
            "bkgrnd": 0
        },
        "t2w": {
            "prefix": "images/t2w/",
            "suffix": ".nii.gz",
            "interp": "linear",
            "bkgrnd": 0
        },
        "tissues": {
            "prefix": "labels/tissues/",
            "suffix": ".nii.gz",
            "labels": "1..9",
            "interp": "nn"
        },
        "structures": {
            "prefix": "labels/structures/",
            "suffix": ".nii.gz",
            "labels": "1..87",
            "interp": "nn"
        }
    },
    "iterations": 8,
    "registration": {
        "affine": {
            "prefix": "global/dof/",
            "suffix": ".dof.gz"
        },
        "config": [
            {
                "energy": "Symmetric",
                "model": "SVFFD",
                "ffdim": "SS",
                "intsteps": 64,
                "liederiv": false,
                "bchterms": 2,
                "channels": ["t2w"],
                "measures": {"t2w": "NMI"},
                "padding": true,
                "interpolation": "Linear with padding",
                "foreground": "Union",
                "levels": 3,
                "resolution": 1.0,
                "spacing": [8.0, 16.0, 32.0],
                "bending": 1e-2,
                "jacobian": 0e-0,
                "blurring": {"tissues": 2, "structures": 2}
            },
            {
                "levels": 4,
                "resolution": 0.5,
                "spacing": [8.0, 8.0, 16.0, 32.0]
            },
            {
                "spacing": 4.0
            },
            {
                "spacing": 4.0
            },
            {
                "spacing": 4.0
            },
            {
                "spacing": 2.0
            },
            {
                "spacing": 2.0
            },
            {
                "spacing": 2.0
            }
        ],
        "growth": {
            "enabled": true,
            "exclavg": false,
            "bchterms": 3
        }
    },
    "regression": {
        "means": [40],
        "sigma": [100],
        "epsilon": 0,
        "precision": 0
    },
    "evaluation": {
        "measures": {
            "t2w": ["mean", "sdev", "gini"],
            "structures": ["label-consistency"]
        },
        "rois": {
            "brain": "{topdir}/global/roi/brain/t{t:02.0f}.nii.gz",
            "roi={l:d}": ["{topdir}/global/roi/t{t:02.0f}/tissue_{l:d}.nii.gz", "1..9"],
            "structure={l:02d}": ["structures", "all"],
            "tissue={l:d}": ["tissues", "all"]
        }
    },
    "environment": {
        "queue": {
            "short": "condor",
            "long": "condor"
        },
        "mintasks": 1,
        "maxtasks": 500,
        "threads": 8,
        "condor": {
            "requirements": [
                "Arch == \"X86_64\"",
                "OpSysShortName == \"Ubuntu\"",
                "OpSysMajorVer >= 14"
            ],
            "environment": {
                "PYTHONPATH": ["/vol/biomedic/users/as12312/lib/python2.7/site-packages"]
            }
        }
    }
}
